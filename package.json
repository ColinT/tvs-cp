{
  "name": "tvs-cp",
  "version": "1.4.0",
  "description": "",
  "main": "./dist/server/app.js",
  "bin": {
    "tvs-cp": "dist/server/app.js"
  },
  "scripts": {
    "start": "node ./dist/server/app.js",
    "build": "rimraf ./dist/* && ng build && ttsc --p ./tsconfig.server.json && png-to-ico ./tvs-cp.png > ./dist/client/tvs-cp.ico",
    "package": "rimraf ./release/* && copyfiles ./patches/**/* ./release && pkg . --options max_old_space_size=4096 --output ./release/tvs-cp-debug.exe && create-nodew-exe ./release/tvs-cp-debug.exe ./release/tvs-cp.exe",
    "release": "rimraf ./dist/* && ng build --prod && ttsc --p ./tsconfig.server.json && png-to-ico ./tvs-cp.png > ./dist/client/tvs-cp.ico && npm run package && cp ./node_modules/memoryjs/build/Release/memoryjs.node ./release/memoryjs.node",
    "postinstall": "npm explore memoryjs -- npm run build64",
    "lint": "eslint ./src --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Colin Tong (https://github.com/ColinT)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ColinT/tvs-cp"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.2",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "^10.2.2",
    "@angular/compiler": "^10.2.2",
    "@angular/core": "^10.2.2",
    "@angular/forms": "^10.2.2",
    "@angular/localize": "^10.2.2",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "^10.2.2",
    "@angular/platform-browser-dynamic": "^10.2.2",
    "@angular/router": "^10.2.2",
    "@ng-bootstrap/ng-bootstrap": "^8.0.0",
    "axios": "^0.21.2",
    "bootstrap": "^4.5.3",
    "cors": "^2.8.5",
    "download-chromium": "^2.2.1",
    "express": "^4.17.3",
    "fast-levenshtein": "^3.0.0",
    "memoryjs": "^3.4.0",
    "popper.js": "^1.16.1",
    "request": "^2.88.0",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "ws": "^7.4.6",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/cli": "^10.2.0",
    "@angular/compiler-cli": "^10.2.2",
    "@angular/language-service": "^10.2.2",
    "@types/express": "^4.17.9",
    "@types/fast-levenshtein": "0.0.1",
    "@types/node": "^14.14.6",
    "@types/ws": "^7.2.9",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "codelyzer": "^6.0.1",
    "copyfiles": "^2.4.0",
    "create-nodew-exe": "^1.0.6",
    "eslint": "^7.13.0",
    "node-gyp": "^7.1.2",
    "pkg": "^4.4.9",
    "png-to-ico": "^2.1.1",
    "rimraf": "^3.0.2",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.0",
    "ttypescript": "^1.5.12",
    "typescript": "~4.0.5",
    "typescript-transform-paths": "^2.0.2"
  },
  "pkg": {
    "assets": [ "./dist/**/*" ],
    "targets": [ "node14-win-x64" ]
  }
}
