{
  "name": "tvs-cp",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/server/app.js",
  "bin": "./dist/server/app.js",
  "scripts": {
    "start": "node ./dist/server/app.js",
    "build": "rimraf ./dist/* && ng build && ttsc --p ./tsconfig.server.json && png-to-ico ./tvs-cp.png > ./dist/client/tvs-cp.ico",
    "package": "rimraf ./release/* && copyfiles ./patches/**/* ./release && pkg . --options max_old_space_size=4096 --output ./release/tvs-cp-debug.exe && create-nodew-exe ./release/tvs-cp-debug.exe ./release/tvs-cp.exe",
    "release": "rimraf ./dist/* && ng build --prod && ttsc --p ./tsconfig.server.json && png-to-ico ./tvs-cp.png > ./dist/client/tvs-cp.ico && npm run package && cp ./node_modules/memoryjs/build/Release/memoryjs.node ./release/memoryjs.node",
    "postinstall": "npm explore memoryjs -- npm run build64",
    "lint": "eslint ./src --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Colin Tong (https://github.com/ColinT)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ColinT/tvs-cp"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.7",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.0.7",
    "@angular/compiler": "^9.0.7",
    "@angular/core": "^9.0.7",
    "@angular/forms": "^9.0.7",
    "@angular/localize": "^9.1.12",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.0.7",
    "@angular/platform-browser-dynamic": "^9.0.7",
    "@angular/router": "^9.0.7",
    "@ng-bootstrap/ng-bootstrap": "^5.3.1",
    "@types/express": "^4.17.8",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.3",
    "cors": "^2.8.5",
    "download-chromium": "^2.2.1",
    "express": "^4.17.1",
    "fast-levenshtein": "^2.0.6",
    "memoryjs": "^3.2.3",
    "request": "^2.88.0",
    "rxjs": "^6.5.5",
    "tslib": "^1.14.1",
    "ws": "^7.3.1",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1002.0",
    "@angular/cli": "^9.0.7",
    "@angular/compiler-cli": "^9.0.7",
    "@angular/language-service": "^9.0.7",
    "@types/fast-levenshtein": "0.0.1",
    "@types/node": "^12.19.3",
    "@types/ws": "^7.2.9",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "awesome-typescript-loader": "^5.2.1",
    "codelyzer": "^5.2.2",
    "copyfiles": "^2.4.0",
    "create-nodew-exe": "^1.0.6",
    "eslint": "^6.8.0",
    "node-gyp": "^6.1.0",
    "pkg": "^4.4.9",
    "png-to-ico": "^2.1.1",
    "rimraf": "^3.0.2",
    "ts-node": "~8.3.0",
    "tslint": "^6.1.3",
    "ttypescript": "^1.5.12",
    "typescript": "~3.7.5",
    "typescript-transform-paths": "^1.1.15"
  },
  "pkg": {
    "assets": [
      "./dist/**/*"
    ],
    "targets": [
      "node10-win-x64"
    ]
  }
}
